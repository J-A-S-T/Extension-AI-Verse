{
  "name": "Cuddly-engima",
  "description": "Cuddly as hell",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "./scripts/background/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./scripts/content/content.js"]
    }
  ],
  "side_panel": {
    "default_path": "index.html"
  },
  "permissions": ["storage", "scripting", "tabs", "sidePanel"]
}
