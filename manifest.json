{
  "name": "Cuddly-engima",
  "description": "Cuddly as hell",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": ["storage", "scripting", "tabs"],
  "background": {
    "service_worker": "./background/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./scripts/content.js"]
    }
  ],
  "action": {
    "default_popup": "index.html"
  }
}
